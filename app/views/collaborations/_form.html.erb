<div class="col-md-8">
  <%= form_for [@wiki, Collaboration.new] do |f| %>
    <table class='table'>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Give Access</th>
      </tr>
      <% @available_collaborators.each do |user| %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= check_box_tag "user_ids[]", user.id %></td>
        </tr>
      <% end %>
    </table>
    <%= f.submit 'Add collaborator', class: 'btn btn-success' %>
  <% end %>
</div>